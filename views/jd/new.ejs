<%- include("../partials/header") %>

<div class="container">
	<div class="row">
		<div class="col-md-6 m-auto">
			<h1 class="text-center my-4">Add a new Job Description</h1>
			<form action="/jd" method="POST">
				<div class="form-group">
					<input class="form-control "type="text" name="title" placeholder="job title">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="jobdesc" placeholder="description">
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block">Submit</button>
				</div>
			</form>
			<a href="/jd">Go back</a>
		</div>
	</div>
</div>

<%- include("../partials/footer") %>